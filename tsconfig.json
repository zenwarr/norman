{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": false,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strict": true,
    "declaration": true,
    "declarationDir": "declarations"
  },
  "exclude": [
    "declarations/**/*.*"
  ]
}
